#include <stdio.h>

int main(void) {
    int T;

    // 입력받기: 테스트 케이스의 개수
    scanf("%d", &T);

    // 각 테스트 케이스 처리
    for(int i = 0; i < T; i++) {
        int A, B;
        // 각 테스트 케이스에서 두 정수 A와 B 입력 받기
        scanf("%d %d", &A, &B);

        // A와 B의 합을 출력하기
        printf("%d\n", A + B);
    }

    return 0;
}
